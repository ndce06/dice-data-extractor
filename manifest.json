{
    "manifest_version": 2,
    "name": "Dice Data Extractor Plugin",
    "description": "This extension will extract job list to CSV from Dice",
    "version": "1.0",
    "browser_action": {
        "default_icon": "icon.png",
        "default_popup": "popup.html"
    },
    "content_scripts": [{
      "matches": ["https://www.dice.com/*"],
      "js": ["myscript.js"]
    }],
    "permissions": [
        "activeTab"
    ]
}
